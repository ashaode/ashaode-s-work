#include<stdio.h>

int getString( char * source , char *strPtr[] ) ;

int main()
{
    char    str[100005];
    char    *strPtr[1005]={0};
    int        i, num ;
    
    gets(str);
    num = getString( str , strPtr ) ;
    for( i = 0 ; i < num ; i++ )
        puts(strPtr[i]);
    
    return 0;    
}

/* 请在这里填写答案 */
#include<string.h>
int getString( char * source , char *strPtr[] ){
	
    int len1=strlen(source);
    int io=0;
    while(source[io]==' '){
    	io++;
	}
	strPtr[0]=&source[io];
	int j=1;
	for(int i=io;i<len1-1;i++){
		if(source[i]==' '&&source[i+1]!=' '){
			strPtr[j]=&source[i+1];
			j++;
		
		}
	}
	for(int i=0;i<len1;i++){
		if(source[i]==' '){
			source[i]='\0';
		}
	}
	return j;


} 
