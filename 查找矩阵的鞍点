#include<stdio.h>
int main(){
	int m,n;
	scanf("%d %d",&m,&n);
	int martix[m][n];
	for(int i=0;i<m;i++){
		for(int j=0;j<n;j++){
			scanf("%d",&martix[i][j]);
		}
	}
	int final_i=0;
	int final_j=0;
	int found=0;
	for(int i=0;i<m;i++){
		int max=martix[i][0];
		for(int j=0;j<n;j++){
			if(max>martix[i][j]){
			//nothing	
			}else{
			max=martix[i][j];
		}
		}
		for(int j=0;j<n;j++){
			if(max==martix[i][j]){
				int temp=max;
				for(int k=0;k<m;k++){
					if(max<=martix[k][j]){
						
					}else{
						max=martix[k][j];
					}
				}
				if(max==temp){
					found=1;
					final_i=i;
					final_j=j;
					break;
					
				} 
				
				
			}
		}
		
		if(found==1){
			break;
		}
	}
	if(found==1){
			printf("The saddle point is (%d,%d)=%d.",final_i,final_j,martix[final_i][final_j]);
		}else{
			printf("There is no saddle point in the matrix.");
		}
	
	return 0;
	
	
} 
