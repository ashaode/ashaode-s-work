#include<stdio.h>
#include<string.h>
int main(){

int n=0;
scanf("%d",&n);//读入学生数量给n 

 
struct student{
	char name[20];
	char id[30];
	int score[5];
};//定义学生结构体 
struct student student[n];//定义结构数组 
int i=0;
for(i=0;i<n;i++){
	int c;
	while((c=getchar())!='\n'&&(c!=EOF)){
		
	}//清空缓冲区 
	fgets(student[i].name,sizeof(student[i].name),stdin);//读入名字 
	size_t len=strlen(student[i].name); 
	if(len>0&&student[i].name[len-1]=='\n'){
		student[i].name[len-1]='\0';
		
	}//去除换行符
	scanf("%s",student[i].id);//读入id 
	int j=0;
	for(j=0;j<5;j++){
		scanf("%d",&student[i].score[j]);
	}//读入各科目成绩 
	
	
}//读入学生数据；

for(i=0;i<n;i++){
double average=0;
double amont=0;
int io;
for(io=0;io<5;io++){
	amont+=student[i].score[io];
}
average=amont/5;
	printf("Name:%s\n",student[i].name);
	printf("ID:%s\n",student[i].id);
	printf("Score:%d %d %d %d %d\n",student[i].score[0],student[i].score[1],student[i].score[2],student[i].score[3],student[i].score[4]);	
	printf("average:%.2f total:%.0f",average,amont);
	printf("\n");
	printf("\n");
	
} 
return 0;
} 
